<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ws.apple.ayuep">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:name=".AYuePApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.navigation.BottomNavigationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.product.ProductListActivity"></activity>
        <activity android:name=".ui.product.ProductDetailActivity"></activity>
        <activity android:name=".ui.order.CreateOrderAcitvity"></activity>
        <activity android:name=".ui.product.PhotoViewActivity"></activity>
        <activity android:name=".ui.order.RegisterCustomerActivity"></activity>
        <activity android:name=".ui.order.CommentActivity"></activity>
        <activity android:name=".ui.order.CommentsActivity"></activity>
        <activity android:name=".ui.store.StoreActivity"></activity>
        <activity android:name=".ui.store.StoreInfoActivity"></activity>
        <activity android:name=".ui.store.StoreProductsActivity"></activity>
        <activity android:name=".ui.store.StoreOrdersActivity"></activity>
        <activity android:name="me.iwf.photopicker.PhotoPickerActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"></activity>
    </application>

</manifest>